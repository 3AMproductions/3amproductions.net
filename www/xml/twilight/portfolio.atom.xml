    <link rel="alternate" type="application/xhtml+xml" href="http://3amproductions.net/portfolio/hbsav2"/>
    <link rel="enclosure" length="3000" type="image/gif" title="screenshot of HBSA v2.0" href="http://3amproductions.net/images/H2big01.gif"/>
